{
  "schemaVersion": 1,
  "id": "${mod_id}",
  "version": "${version}",
  "name": "${mod_name}",
  "description": "${mod_description}",
  "authors": [
    "${mod_author}"
  ],
  "contact": {
    "homepage": "${mod_url}",
    "sources": "${mod_url}",
    "issues": "${mod_issue_tracker}"
  },
  "license": "${mod_license}",
  "icon": "logo.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "de.cech12.brickhopper.FabricBrickHopperMod"
    ],
    "client": [
      "de.cech12.brickhopper.client.FabricBrickHopperClientMod"
    ],
    "modmenu": [
      "de.cech12.brickhopper.compat.ModMenuCompat"
    ]
  },
  "depends": {
    "java": ">=${java_version}",
    "minecraft": ">=${minecraft_version}",
    "fabricloader": ">=${fabric_loader_version}",
    "fabric-api": ">=${fabric_version}",
    "cloth-config": ">=${cloth_config_version}"
  },
  "suggests": {
    "modmenu": ">=${mod_menu_version}"
  }
}
